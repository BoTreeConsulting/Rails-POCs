<h2>Create new user</h2>
<%= form_for @user do |f| %>
 <div>
  <%= f.label :name, 'User Name' %>
  <%= f.text_field :name %>
 </div>
 
 <div>
  <%= f.label :phone, 'Phone number' %>
  <%= f.text_field :phone %>
 </div>
 
 <div>
  <%= f.label :email, 'Email address' %>
  <%= f.text_field :email %>
 </div>
 
 <div>
  <%= f.label :resume, 'Upload Resume' %>
  <%= f.file_field :resume %>
 </div>
 
 <%= f.submit :method=>:create %>
<% end %>

<%= link_to "Back", user_path %>
